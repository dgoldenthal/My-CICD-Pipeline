{
  "name": "coding-quiz",
  "version": "1.0.0",
  "description": "coding-quiz-app",
  "scripts": {
    "start": "cd server && npm run start",
    "seed": "cd server && npm run seed",
    "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm run dev\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "build": "concurrently \"cd server && npm run build\" \"cd client && npm run build\"",
    "render-build": "npm install && npm run build",
    "test": "npx cypress run --browser electron",
    "test-component": "npx cypress run --component",
    "test:cypress": "cd client && npm run test:cypress",
    "test-gui": "npx cypress open"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@cypress/react": "^9.0.0",
    "@cypress/vite-dev-server": "^6.0.2",
    "concurrently": "^8.2.0",
    "cypress": "^14.0.1",
    "mochawesome": "^7.1.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-node": "^10.9.2",
    "vite": "^5.4.14"
  },
  "dependencies": {
    "@cypress/react18": "^2.0.1",
    "@types/prop-types": "^15.7.14",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.1",
    "vitest": "^2.0.3"
  }
}
